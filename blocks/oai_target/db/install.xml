<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="blocks/oai_target/db" VERSION="20101223" COMMENT="XMLDB file for Moodle blocks/oai_target"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:noNamespaceSchemaLocation="../../../lib/xmldb/xmldb.xsd"
>
    <TABLES>
        <TABLE NAME="block_oai_target_courses" NEXT="block_oai_target_log" COMMENT="Courses table">
            <FIELDS>
                <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="true" NEXT="course_id"/>
                <FIELD NAME="course_id" TYPE="int" LENGTH="11" NOTNULL="false" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" COMMENT="The course identificator" PREVIOUS="id" NEXT="last_update_time"/>
                <FIELD NAME="last_update_time" TYPE="int" LENGTH="11" NOTNULL="false" UNSIGNED="true" DEFAULT="0" SEQUENCE="false" COMMENT="The time of the last the update" PREVIOUS="course_id" NEXT="notify_by_pmh"/>
                <FIELD NAME="notify_by_pmh" TYPE="int" LENGTH="1" NOTNULL="false" UNSIGNED="true" DEFAULT="1" SEQUENCE="false" COMMENT="Enables PMH" PREVIOUS="last_update_time" NEXT="update_frequency"/>
                <FIELD NAME="update_frequency" TYPE="int" LENGTH="11" NOTNULL="false" UNSIGNED="true" DEFAULT="43200" SEQUENCE="false" PREVIOUS="notify_by_pmh" />
            </FIELDS>
            <KEYS>
                <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
            </KEYS>
        </TABLE>
        <TABLE NAME="block_oai_target_log" COMMENT="block_oai_target_log table retrofitted from MySQL" NEXT="block_oai_target_lom_records" PREVIOUS="block_oai_target_courses">
            <FIELDS>
                <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="true" NEXT="course_id"/>
                <FIELD NAME="course_id" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="false" PREVIOUS="id" NEXT="module_id"/>
                <FIELD NAME="module_id" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="false" PREVIOUS="course_id" NEXT="name"/>
                <FIELD NAME="name" TYPE="char" LENGTH="255" NOTNULL="true" SEQUENCE="false" PREVIOUS="module_id" NEXT="type"/>
                <FIELD NAME="type" TYPE="char" LENGTH="255" NOTNULL="true" DEFAULT="resource" SEQUENCE="false" PREVIOUS="name" NEXT="action"/>
                <FIELD NAME="action" TYPE="char" LENGTH="7" NOTNULL="true" DEFAULT="added" SEQUENCE="false" PREVIOUS="type" NEXT="status"/>
                <FIELD NAME="status" TYPE="char" LENGTH="8" NOTNULL="true" DEFAULT="notified" SEQUENCE="false" PREVIOUS="action"/>
            </FIELDS>
            <KEYS>
                <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
            </KEYS>
        </TABLE>
        <TABLE NAME="block_oai_target_lom_records" COMMENT="block_oai_target_lom_records table retrofitted from MySQL" PREVIOUS="block_oai_target_log">
            <FIELDS>
                <FIELD NAME="id" TYPE="int" LENGTH="11" NOTNULL="true" UNSIGNED="true" SEQUENCE="true" NEXT="provider"/>
                <FIELD NAME="provider" TYPE="char" LENGTH="255" NOTNULL="false" UNSIGNED="true" SEQUENCE="false" PREVIOUS="id" NEXT="url"/>
                <FIELD NAME="url" TYPE="char" LENGTH="255" NOTNULL="true" UNSIGNED="true" SEQUENCE="false" PREVIOUS="provider" NEXT="enterdate"/>
                <FIELD NAME="enterdate" TYPE="datetime" NOTNULL="true" SEQUENCE="false" PREVIOUS="url" NEXT="oai_identifier"/>
                <FIELD NAME="oai_identifier" TYPE="char" LENGTH="255" NOTNULL="false" SEQUENCE="false" PREVIOUS="enterdate" NEXT="oai_set"/>
                <FIELD NAME="oai_set" TYPE="char" LENGTH="255" NOTNULL="false" SEQUENCE="false" PREVIOUS="oai_identifier" NEXT="datestamp"/>
                <FIELD NAME="datestamp" TYPE="datetime" NOTNULL="true" SEQUENCE="false" PREVIOUS="oai_set" NEXT="deleted"/>
                <FIELD NAME="deleted" TYPE="char" LENGTH="5" NOTNULL="false" DEFAULT="None" SEQUENCE="false" PREVIOUS="datestamp" NEXT="dc_title"/>
                <FIELD NAME="dc_title" TYPE="char" LENGTH="1333" NOTNULL="false" SEQUENCE="false" PREVIOUS="deleted" NEXT="dc_creator"/>
                <FIELD NAME="dc_creator" TYPE="text" NOTNULL="false" SEQUENCE="false" PREVIOUS="dc_title" NEXT="dc_subject"/>
                <FIELD NAME="dc_subject" TYPE="char" LENGTH="255" NOTNULL="false" SEQUENCE="false" PREVIOUS="dc_creator" NEXT="dc_description"/>
                <FIELD NAME="dc_description" TYPE="text" NOTNULL="false" SEQUENCE="false" PREVIOUS="dc_subject" NEXT="dc_contributor"/>
                <FIELD NAME="dc_contributor" TYPE="char" LENGTH="255" NOTNULL="false" SEQUENCE="false" PREVIOUS="dc_description" NEXT="dc_publisher"/>
                <FIELD NAME="dc_publisher" TYPE="char" LENGTH="255" NOTNULL="false" SEQUENCE="false" PREVIOUS="dc_contributor" NEXT="dc_date"/>
                <FIELD NAME="dc_date" TYPE="datetime" NOTNULL="true" SEQUENCE="false" PREVIOUS="dc_publisher" NEXT="dc_type"/>
                <FIELD NAME="dc_type" TYPE="char" LENGTH="255" NOTNULL="false" SEQUENCE="false" PREVIOUS="dc_date" NEXT="dc_format"/>
                <FIELD NAME="dc_format" TYPE="char" LENGTH="255" NOTNULL="false" SEQUENCE="false" PREVIOUS="dc_type" NEXT="dc_identifier"/>
                <FIELD NAME="dc_identifier" TYPE="char" LENGTH="255" NOTNULL="false" SEQUENCE="false" PREVIOUS="dc_format" NEXT="dc_source"/>
                <FIELD NAME="dc_source" TYPE="char" LENGTH="255" NOTNULL="false" SEQUENCE="false" PREVIOUS="dc_identifier" NEXT="dc_language"/>
                <FIELD NAME="dc_language" TYPE="char" LENGTH="255" NOTNULL="false" SEQUENCE="false" PREVIOUS="dc_source" NEXT="dc_relation"/>
                <FIELD NAME="dc_relation" TYPE="char" LENGTH="255" NOTNULL="false" SEQUENCE="false" PREVIOUS="dc_language" NEXT="dc_coverage"/>
                <FIELD NAME="dc_coverage" TYPE="char" LENGTH="255" NOTNULL="false" SEQUENCE="false" PREVIOUS="dc_relation" NEXT="dc_rights"/>
                <FIELD NAME="dc_rights" TYPE="char" LENGTH="255" NOTNULL="false" SEQUENCE="false" PREVIOUS="dc_coverage" NEXT="lom_record"/>
                <FIELD NAME="lom_record" TYPE="text" NOTNULL="true" SEQUENCE="false" PREVIOUS="dc_rights" />
            </FIELDS>
            <KEYS>
                <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
            </KEYS>
        </TABLE>
    </TABLES>
</XMLDB>
